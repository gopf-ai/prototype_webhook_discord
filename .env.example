# Get these from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here

# OAuth2 (for self-service user authorization)
# Go to OAuth2 → General → Reset Secret to get the client secret
DISCORD_CLIENT_SECRET=your_client_secret_here
# Must match the redirect URI registered in Developer Portal → OAuth2 → Redirects
DISCORD_REDIRECT_URI=http://localhost:8501
